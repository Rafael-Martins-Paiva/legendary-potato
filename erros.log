public class MediaNotas {

    public static void main(String[] args) {
        // Vetor com as notas de exemplo
        double[] notas = {8.5, 7.0, 9.2, 6.5, 10.0, 8.8};
        
        // Variável para armazenar a soma das notas
        double soma = 0;

        // For-each loop para somar as notas
        for (double nota : notas) {
            soma += nota;
        }

        // Calcula a média dividindo a soma pelo número de elementos no vetor
        double media = soma / notas.length;

        // Imprime o resultado formatado
        System.out.printf("A média das notas é: %.2f%n", media);
    }
}
