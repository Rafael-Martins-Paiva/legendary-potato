public class MultiplicationTable {

    public static void main(String[] args) {
        System.out.println("Tabuada do 1 ao 10\n");

        // Loop externo para os números de 1 a 10
        for (int i = 1; i <= 10; i++) {
            System.out.println("--- Tabuada do " + i + " ---");

            // Loop interno para a multiplicação de 1 a 10
            for (int j = 1; j <= 10; j++) {
                // Imprime a operação e o resultado
                System.out.println(i + " x " + j + " = " + (i * j));
            }
            System.out.println(); // Pula uma linha para separar as tabuadas
        }
    }
}
